"    <xsl:text>&nl;&nl;</xsl:text>\n"
"    <xsl:apply-templates select=\"nets/net\"/>          <!-- Generate list of nets and connections -->\n"
"    <xsl:text>&nl;.END&nl;</xsl:text>\n"
"</xsl:template>\n"
